{
    "manifest_version": 3,
    "name": "Bookmark Manager",
    "version": "1.0",
    "description": "A Chrome extension to manage bookmarks",
    "permissions": ["bookmarks"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  